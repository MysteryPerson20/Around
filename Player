local Players = game:GetService("Players")

local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

local function teleportInCircle()
    local originalPosition = humanoidRootPart.Position
    local radius = 5 -- Small radius to go around the character
    local segments = 12 -- Number of segments in the circle
    local delayTime = 0.001 -- Extremely fast teleportation

    for i = 0, segments - 1 do
        local angle = math.rad((i / segments) * 360)
        
        -- Calculate the new position in the circle around the character
        local offsetX = radius * math.cos(angle)
        local offsetZ = radius * math.sin(angle)
        local newPosition = originalPosition + Vector3.new(offsetX, 0, offsetZ)
        
        -- Teleport to the new position
        humanoidRootPart.CFrame = CFrame.new(newPosition)
        
        -- Wait before teleporting to the next position
        wait(delayTime)
    end

    -- Teleport back to the original position
    humanoidRootPart.CFrame = CFrame.new(originalPosition)
end

teleportInCircle()
